{
  "name": "iso-639-to-heb",
  "version": "1.0.0",
  "description": "Converts iso-639 codes to the full Hebrew language name",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "hebrew",
    "heb",
    "iso-639",
    "iso-639-1",
    "language",
    "codes",
    "code",
    "names"
  ],
  "author": "Sagi Shalom",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.5"
  }
}
